// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Users {
  id Int @id @default(autoincrement())
  firstname String
  lastname String
  age Int
  email String @unique
  password String
  status String
  user_type String
}
